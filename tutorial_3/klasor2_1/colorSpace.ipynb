    "\n",
    "img = cv2.imread(r\"materyal\\colors.png\")\n",
    "\n",
    "B,G,R = cv2.split(img)\n",
    "\n",
    "cv2.imshow(\"Red\", R)\n",
    "cv2.imshow(\"Green\", G)\n",
    "cv2.imshow(\"Blue\", B)\n",
    "cv2.imshow(\"Original\", img)\n",
    "\n",
    "cv2.waitKey(0)\n",
    "cv2.destroyAllWindows()"
   ]
